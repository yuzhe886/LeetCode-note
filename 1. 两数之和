class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int> m;
        for(int i=0;i<nums.size();i++){
            m[nums[i]]=i;
        }
        for(int i=0;i<nums.size();i++){
            if(m.count(target-nums[i])&&m[target-nums[i]]!=i){
                if(i<m[target-nums[i]]) return{i,m[target-nums[i]]};
                else return {m[target-nums[i]],i};
            }
        }
        return {};
    }
};
//学会了unordered_map,map.count
